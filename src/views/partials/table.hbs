<table class='table'>
  <thead>
    <tr>
      <th scope='col'>ID</th>
      <th scope='col'>Titulo</th>
      <th scope='col'>Precio</th>
      <th scope='col'>Foto</th>
    </tr>
  </thead>
  <tbody id="table">
    {{#each comics}}
      <tr>
        <th scope='row'>{{this.id}}</th>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td>
          <img src={{this.thumbnail}} alt='' width='50' />
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>